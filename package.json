{
  "name": "expobasicapp",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint-errors": "prettier -c --write .  && eslint . --ext .js,.jsx,.ts,.tsx --fix --quiet",
    "lint": "prettier -c --write . && eslint . --ext .js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "clear-full-cache": "rm -rf node_modules && yarn cache clean && watchman watch-del-all && rm -fr $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-cache && yarn",
    "clear-full-cache-ci": "rm -rf node_modules && yarn cache clean && rm -fr $TMPDIR/haste-map-* && rm -rf $TMPDIR/metro-cache && yarn"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
